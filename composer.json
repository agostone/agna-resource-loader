{
  "name": "agna/resource-loader",
  "description": "pluggable resource loader",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Agoston Nagy",
      "email": "agoston.nagy@use.github.please"
    }
